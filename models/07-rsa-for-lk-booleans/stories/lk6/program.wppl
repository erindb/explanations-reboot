var program = function(input) {

	var causeAB = true;
	var casueBC = true;

	var A = input.A;
	var Mab = input.Mab;
	var Mbc = input.Mbc;
	
	var B = causeAB ? (Mab ? A : !A) : false;
	var C = causeBC ? (Mbc ? B : !B) : true;

