// B because D
// could have been no causal link B->C OR C->D

var program = function(input) {

	var causeBC = true;
	var cuaseCD = true;

	var A = input.A;
	var B = input.B;
	
	var C = causeBC ? (A || B) : A;
	var D = causeCD ? C : false;

	return {
		A: A,
		B: B,
		C: C,
		D: D
	};
};

var input = {
	A: false,
	B: false
};

program(input);
