// B because C
// could have been no causal link A,B -> C

var program = function(input) {

	var cause = true;

	var xor = function(a,b) {
		return (a || b) && !(a && b);
	};

	var A = input.A;
	var B = input.B;
	var C = cause ? xor(A, B) : false;

	return {
		A: A,
		B: B,
		C: C
	};
};

var input = {
	A: true,
	B: true
};

program(input);
