// C because D
// could have been no causal link C->D

var program = function(input) {

	var cause = true;

	var A = input.A;
	var B = input.B;

	var ac = true;
	var bc = input.bc;

	var C = (A && ac) ? true : (B && bc);
	var D = cause ? (C && input.cd) : true;

	return {
		A: A,
		B: B,
		C: C,
		D: D
	};
};

var input = {
	A: true,
	B: true,
	bc: true
};

program(input);
