// C because D
// could have been no causal link C->D

var program = function(input) {

	var cause = true;

	var A = input.A;
	var B = input.B;
	
	var C = A || B;
	var D = cause ? (C && input.cd) : false;

	return {
		A: A,
		B: B,
		C: C,
		D: D
	};
};

var input = {
	A: false,
	B: false
};

program(input);
