// A because B
// could have been B->A

var program = function(input) {

	var cause = false;

	var ignored = false;

	var B = input.B;
	var A = cause ? B : input.A;

	var unobservables = {
		structureParams: ["ignored"],
		input: []
	};

	return {
		A: A,
		B: B
	};
};

var input = {
	A: true,
	B: true
};

program(input);
