// A because C
// could have been no causal link A->C

var program = function(input) {

	var causeAC = true;

	var A = input.A;
	var B = input.B;

	var ac = true;
	var bc = input.bc;

	var C = (causeAC ?
		((A&&ac)?true:(B&&bc)) :
		(B&&bc)
	);
	var D = (C && input.cd);

	return {
		A: A,
		B: B,
		C: C,
		D: D
	};
};

var input = {
	A: true,
	B: true,
	bc: true
};

program(input);
