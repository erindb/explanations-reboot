// A because B
// could have been no causal link A->B

var program = function(input) {

	var causeAB = true;

	var A = input.A;
	var Mab = input.Mab;
	var Mbc = input.Mbc;
	
	var B = causeAB ? (Mab ? A : !A) : false;
	var C = Mbc ? B : !B;

	return {
		A: A,
		B: B,
		C: C
	}
};

var input = {
	A: false,
	Mab: true,
	Mbc: false
};

program(input);
