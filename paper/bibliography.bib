@article{machery_folk_2008,
	title = {The {Folk} {Concept} of {Intentional} {Action}: {Philosophical} and {Experimental} {Issues}},
	volume = {23},
	issn = {1468-0017},
	shorttitle = {The {Folk} {Concept} of {Intentional} {Action}},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1468-0017.2007.00336.x/abstract},
	doi = {10.1111/j.1468-0017.2007.00336.x},
	abstract = {Abstract:  Recent experimental findings by Knobe and others (Knobe, 2003; Nadelhoffer, 2006b; Nichols and Ulatowski, 2007) have been at the center of a controversy about the nature of the folk concept of intentional action. I argue that the significance of these findings has been overstated. My discussion is two-pronged. First, I contend that barring a consensual theory of conceptual competence, the significance of these experimental findings for the nature of the concept of intentional action cannot be determined. Unfortunately, the lack of progress in the philosophy of concepts casts doubt on whether such a consensual theory will be found. Second, I propose a new, deflationary interpretation of these experimental findings, ‘the trade-off hypothesis’, and I present several new experimental findings that support this interpretation.},
	language = {en},
	number = {2},
	urldate = {2016-12-12},
	journal = {Mind \& Language},
	author = {Machery, Edouard},
	month = apr,
	year = {2008},
	pages = {165--189},
	file = {Full Text PDF:/Users/erindb/Library/Application Support/Zotero/Profiles/v618i0z7.default/zotero/storage/TWRHCDA6/Machery - 2008 - The Folk Concept of Intentional Action Philosophi.pdf:application/pdf;Snapshot:/Users/erindb/Library/Application Support/Zotero/Profiles/v618i0z7.default/zotero/storage/KTCABCWJ/abstract.html:text/html}
}

