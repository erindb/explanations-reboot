@article{lucas_improved_2015,
	title = {An improved probabilistic account of counterfactual reasoning},
	volume = {122},
	copyright = {(c) 2016 APA, all rights reserved},
	issn = {1939-1471 0033-295X},
	doi = {10.1037/a0039655},
	abstract = {When people want to identify the causes of an event, assign credit or blame, or learn from their mistakes, they often reflect on how things could have gone differently. In this kind of reasoning, one considers a counterfactual world in which some events are different from their real-world counterparts and considers what else would have changed. Researchers have recently proposed several probabilistic models that aim to capture how people do (or should) reason about counterfactuals. We present a new model and show that it accounts better for human inferences than several alternative models. Our model builds on the work of Pearl (2000), and extends his approach in a way that accommodates backtracking inferences and that acknowledges the difference between counterfactual interventions and counterfactual observations. We present 6 new experiments and analyze data from 4 experiments carried out by Rips (2010), and the results suggest that the new model provides an accurate account of both mean human judgments and the judgments of individuals.},
	language = {English},
	number = {4},
	journal = {Psychological Review},
	author = {Lucas, Christopher G. and Kemp, Charles},
	year = {2015},
	keywords = {*Causality, *Reasoning, Judgment},
	pages = {700--734},
	file = {APA Psycnet Fulltext PDF:/Users/erindb/Library/Application Support/Zotero/Profiles/v618i0z7.default/zotero/storage/5PIMTSC7/Lucas and Kemp - 2015 - An improved probabilistic account of counterfactua.pdf:application/pdf}
}

